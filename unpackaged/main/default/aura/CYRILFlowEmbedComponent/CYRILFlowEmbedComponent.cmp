<aura:component description="Flow Embed Component" implements="forceCommunity:availableForAllPageTypes,force:hasRecordId,force:lightningQuickAction,lightning:isUrlAddressable,flexipage:availableForAllPageTypes" access="global">

    <aura:handler name="init" value="{!this}" action="{!c.init}" />
    <aura:attribute name="flowName" type="String" />
    <aura:attribute name="rID" type="String" />
    <aura:attribute name="rIDName" type="String" />
    <aura:attribute name="redirectVar" type="String" />
    <aura:attribute name="redirectVarVal" type="String" />
    <aura:attribute name="redirectVarValName" type="String" />
    <aura:attribute name="redirectPath" type="String" />
    <lightning:flow aura:id="flowData" onstatuschange="{!c.handleStatusChange}"/>

</aura:component>