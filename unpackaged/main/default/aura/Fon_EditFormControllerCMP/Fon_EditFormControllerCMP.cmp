<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,lightning:isUrlAddressable" access="global" 
                controller="Fon_EditFormController">
	<aura:attribute name="lookupFieldAPIVar" type="string"/>
	<aura:attribute name="recordPageObjectvar" type="string"/>
	<aura:attribute name="fieldSetVar" type="string"/>
    
    <aura:attribute name="soRecordId" type="string"/>
    <aura:attribute name="recordId" type="Id" />
    
    <aura:attribute name="isRecordIdFoundVar" type="Boolean" default="false"/>
    <aura:attribute name="objFieldSet" type="List" access="private"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <lightning:notificationsLibrary aura:id="notificationsLibrary"/>
    <aura:if isTrue="{! not( empty( v.soRecordId ) ) }">
    	<lightning:recordViewForm aura:id="recordViewForm"
                              recordId="{!v.soRecordId}"
                              objectApiName="OrderApi__Sales_Order__c"
                              density="comfy"
                              >
            <div class="slds-box">12
                 <aura:iteration items="{!v.objFieldSet}" var="field" >
                     <lightning:outputField aura:id="field"  fieldName="{!field.fieldName}"/>
                  </aura:iteration>   
            </div>
         
    	</lightning:recordViewForm>
    </aura:if>
    
</aura:component>